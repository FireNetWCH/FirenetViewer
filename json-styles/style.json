{
  "QtBinding": "PySide6",
  "ShowLogs": true,
  "LiveCompileQss": true,
  "CheckForMissingicons": true,
  "QStyleSheetClasses": [
        {
            "style": "#customButton { background-color: #FFFFFF }"
        }
    ],
  
  "QMainWindow": [
    {
      "tittle": "FirenetViewer",
      "icon": ":/font_awesome_brands/icons/font_awesome/brands/brave-reverse.png",
      "frameless": true,
      "transluscentBg": true,
      "sizeGrip": "sizeGrip",
      "navigation": [
        {
          "minimize": "minimizeBtn",
          "close": "closeBtn",
          "moveWindow": "centralwidget",
          "tittleBar": "header",
          "restore": [
            {
            "buttonName": "restoreBtn",
            "normalIcon": ":/feather/Icons/feather/square.png",
            "maximizedIcon": ":/feather/Icons/feather/copy.png"
            }
          ]
        }
      ]
    }
  ],
  
  "QSettings": [
    {
      "ThemeSettings": [
        {
          "CustomTheme": [
            {
              "Background-color": "#FFFFFF",
              "Text-color": "#010000",
              "Icons-color": "#000000",
              "Accent-color": "#26bae3",
              "Theme-name": "LightBlue",
              "Default-Theme": true
            },
            {
              "Background-color": "#21272a",
              "Text-color": "#fefefe",
              "Icons-color": "#fefefe",
              "Accent-color": "#fba43b",
              "Theme-name": "DarkYellow",
              "Default-Theme": false
            }
          ]
        }
      ],
      "AppSettings": {
        "OrginizationName": "FirenetViewer",
        "ApplicationName": "firenet",
        "OrginizationDormain": "firenet.com.pl"
      }
    }
  ],
  "QPushButtonGroup": [
	  {
		"Buttons": [
		  "homeBtn",
		  "dataBtn",
		  "reportsBtn",
		  "graphsBtn"
		],
		"ActiveButton": "homeBtn",
		"Style": [
		  {
			"Active": "QPushButton{background-color: #102339;border-radius: 0px; font-weight: bold; border-top: 2px solid #102339; border-left: 5px solid #ffffff;}",
			"NotActive": "QPushButton{background-color: #102339;border-radius: 0px; font-weight: normal; border-top: 2px solid transparent; border-left: 5px solid transparent;border-bottom: 6px solid transparent;}"
		  }
		]
	  },
	  {
		"Buttons": [
		  "settingsBtn",
		  "infoBtn",
		  "helpBtn",
		  "fileBtn"
		
		],
		"ActiveButton": "fileBtn",
		"Style": [
		  {
			"Active": "QPushButton{background-color: #102339;border-radius: 0px; font-weight: bold; border-top: 2px solid #102339; border-left: 5px solid #ffffff;}",
			"NotActive": "QPushButton{background-color: #102339;border-radius: 0px; font-weight: normal; border-top: 2px solid transparent; border-left: 5px solid transparent;border-bottom: 6px solid transparent;}"

		  }
		]
	  },
	 
	  {
		"Buttons": [
		  "notificationBtn",
		  "moreBtn",
		  "profileBtn",
		  "closeRightMenuBtn",
		  "startDataBtn",
		  "showSearchPanelBtn"

		],
		"Style": [
		  {
			"Active": "QPushButton{background-color: THEME.COLOR_BACKGROUND_2; border-radius: 5px} QPushButton::hover{background-color: THEME.COLOR_ACCENT_1}",
			"NotActive": "QPushButton{background-color: transparent; font-weight: normal; border-radius: 5px} QPushButton::hover{background-color: THEME.COLOR_ACCENT_1}"
		  }
		]
	  }
	
	],
		  
  "QCustomSlideMenu": [
	  {
		"name": "leftMenu",
		"Background-color":"#00ff00",
		"color":"#00ff00",
		"background-color": "#00ff00",
		"style": {
            "whenMenuIsCollapsed": {
              "background-color": "#00ff00",
              "color": "#00ff00"
            },
            "whenMenuIsExpanded": {
              "background-color": "#00ff00",
              "color": "#00ff00"
            }},
		"defaultSize": [
		  {
			"width": 40,
			"height": "parent"
		  }
		],
		"collapsedSize": [
		  {
			"width": 40,
			"height": "parent"
		  }
		],
		"expandedSize": [
		  {
			"width": "auto",
			"height": "parent"
		  }
		],
		
		"toggleButton": [
		  {
			"buttonName": "menuBtn",
			"icons": [
			  {
				"whenMenuIsCollapsed": ":/feather/icons/feather/align-justify.png",
				"whenMenuIsExpanded": ":/feather/icons/feather/arrow-left-circle.png"
			  }
			]
		  }
		],
		"menuTransitionAnimation": [
		  {
			"animationDuration": 500,
			"animationEasingCurve": "Linear"
		  }
		]
	  },
    {
      "name": "centerMenu",
      "defaultSize": [
        {
        "width": 0,
        "height": "parent"
        }
      ],
      "collapsedSize": [
        {
        "width": 0,
        "height": "parent"
        }
      ],
      "expandedSize": [
        {
        "width": "auto",
        "height": "parent"
        }
      ],
      "menuTransitionAnimation": [
        {
        "animationDuration": 500,
        "animationEasingCurve": "Linear"
        }
      ]
    },
    {
      "name": "rightMenu",
      "defaultSize": [
        {
        "width": 0,
        "height": "parent"
        }
      ],
      "collapsedSize": [
        {
        "width": 0,
        "height": "parent"
        }
      ],
      "expandedSize": [
        {
        "width": 200,
        "height": "parent"
        }
      ],
      "menuTransitionAnimation": [
        {
        "animationDuration": 500,
        "animationEasingCurve": "Linear"
        }
      ]
    }
  ],
  "QCustomQStackedWidget": [
	  {
		"name": "mainPages",
		"navigation": [
		  {
			"navigationButtons": [
			  {
				"homeBtn": "homePage",
				"dataBtn": "dataAnalysisPage",
				"reportsBtn": "reportsPage",
				"graphsBtn": "chartPage"
			  }
			]
		  }
		],
		"transitionAnimation": [
		  {
			"fade": [
			  {
				"active": true,
				"duration": 500,
				"easingCurve": "InSine"
			  }
			]
		  }
		]
	  },
	  {
		"name": "centerMenuPages",
		"navigation": [
		  {
			"navigationButtons": [
			  {
				"settingsBtn": "settingsPage",
				"infoBtn": "informationPage",
				"fileBtn": "fileReader",
				"meilBoxBtn": "helpPage"
			  }
			]
		  }
		],
    "transitionAnimation": [
		  {
			"fade": [
			  {
				"active": true,
				"duration": 500,
				"easingCurve": "InSine"
			  }
			],
      "slide": [
			  {
				"active": true,
				"duration": 500,
				"easingCurve": "InSine"
			  }
			]
		  }
		]
	  },
	  {
		"name": "rightMenuPages",
		"navigation": [
		  {
			"navigationButtons": [
			  {
				"notificationBtn": "notificationsPage",
				"moreBtn": "headerEmailWiget",
				"profileBtn": "profilePage"
			  }
			]
		  }
		],
    "transitionAnimation": [
		  {
			"slide": [
			  {
				"active": true,
				"duration": 500,
				"easingCurve": "InSine"
			  }
			],
      "fade": [
			  {
				"active": true,
				"duration": 500,
				"easingCurve": "InSine"
			  }
			]
		  }
		]
	  }
	]
	
}